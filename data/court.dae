  <asset>
    <up_axis>Y_UP</up_axis>
  </asset>
  <library_images>
    <image id="Image-Court">
      <init_from>court.png</init_from>
    </image>
  </library_images>
  <library_effects>
    <effect id="Effect-Court">
      <profile_COMMON>
        <newparam sid="TextureSurface-Court">
          <surface type="2D">
            <init_from>Image-Court</init_from>
            <format>A8R8G8B8</format>
          </surface>
        </newparam>
        <newparam sid="TextureSampler-Court">
          <sampler2D>
            <source>TextureSurface-Court</source>
            <minfilter>LINEAR_MIPMAP_LINEAR</minfilter>
            <magfilter>LINEAR</magfilter>
          </sampler2D>
        </newparam>
        <technique>
          <!-- <phong>
            <ambient>
              <color>0.5 0.5 0.5 1.0</color>
            </ambient>
            <diffuse>
              <texture texcoord="UVSET0" texture="TextureSampler-Court" />
            </diffuse>
            <specular>
              <color>0.1 0.1 0.1 1.0</color>
            </specular>
            <shininess>
              <float>1.0</float>
            </shininess>
	  </phong> -->
          <lambert>
            <ambient>
              <color>0.5 0.5 0.5 1.0</color>
            </ambient>
            <diffuse>
              <texture texcoord="UVSET0" texture="TextureSampler-Court" />
            </diffuse>
          </lambert>
        </technique>
      </profile_COMMON>
    </effect>
    <effect id="Effect-Shadow">
      <profile_COMMON>
        <technique>
          <phong>
            <ambient>
              <color>0.0 0.0 0.0 1.0</color>
            </ambient>
            <diffuse>
              <color>0.0 0.0 0.0 1.0</color>
            </diffuse>
            <specular>
              <color>0.0 0.0 0.0 1.0</color>
            </specular>
            <shininess>
              <float>0.0</float>
            </shininess>
          </phong>
        </technique>
      </profile_COMMON>
    </effect>
    <effect id="Effect-Ball">
      <profile_COMMON>
        <technique>
          <phong>
            <ambient>
              <color>1.0 1.0 0.0 1.0</color>
            </ambient>
            <diffuse>
              <color>1.0 1.0 0.0 1.0</color>
            </diffuse>
            <specular>
              <color>0.5 0.5 0.0 1.0</color>
            </specular>
            <shininess>
              <float>5.0</float>
            </shininess>
          </phong>
        </technique>
      </profile_COMMON>
    </effect>
  </library_effects>
  <library_materials>
    <material id="Material-Court">
      <instance_effect url="#Effect-Court" />
    </material>
    <material id="Material-Shadow">
      <instance_effect url="#Effect-Shadow" />
    </material>
    <material id="Material-Ball">
      <instance_effect url="#Effect-Ball" />
    </material>
  </library_materials>
